{
  "configuration": {
    "status": "error",
    "name": "SISTCI_Log",
    "packages": "com.github.gtmelo.sistci_api",
    "ThresholdFilter": {
      "level": "debug"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%highlight{[%p][%-5c{2}][%-5d{ISO8601}]} %-5m%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "ALL",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}